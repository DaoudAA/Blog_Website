{% extends 'base.html.twig' %}
{% block body %}  
    <h1> Inscription sur le site </h1>  
    {{form_start(form)}}  
    {{form_row(form.name)}}
    {{form_row(form.email)}}
    {{form_row(form.phone)}}   
    {{form_row(form.content)}} 
    <button class="btn btn-success" type="submit">     
        Ajouter votre message !     </button>     
    {{form_end(form)}} 
{% endblock %}